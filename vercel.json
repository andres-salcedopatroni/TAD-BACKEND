{
    "builds": [
        {
            "src": "package.json",
            "use": "vercel-sapper"
        },
      {
        "src": "./bin/www.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./bin/www.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
            "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }